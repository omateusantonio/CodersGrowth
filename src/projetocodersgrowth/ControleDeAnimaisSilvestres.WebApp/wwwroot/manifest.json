{
	"_version": "1.58.0",
	"sap.app": {
		"id": "ui5.controledeanimaissilvestres",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.controledeanimaissilvestres.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			}
		},
		"rootView": {
			"viewName": "ui5.controledeanimaissilvestres.view.App",
			"type": "XML",
			"id": "app"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "ui5.controledeanimaissilvestres.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "listaAnimaisSilvestres",
					"target": "listaAnimaisSilvestres"
				},
				{
					"pattern": "{nomeDoAnimalDetalhado}/{idDoAnimalDetalhado}",
					"name": "paginaDeDetalhesDoAnimal",
					"target": "paginaDeDetalhesDoAnimal"
				}
			],
			"targets": {
				"listaAnimaisSilvestres": {
					"id": "listaAnimaisSilvestres",
					"name": "ListaAnimaisSilvestres"
				},
				"paginaDeDetalhesDoAnimal": {
					"id": "paginaDeDetalhesDoAnimal",
					"name": "PaginaDeDetalhesDoAnimal"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}